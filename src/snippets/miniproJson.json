{
  "//": "感谢https://github.com/ChandZhang/wechat-snippet-vscode/edit/master/snippets/wxjson.json支持",
  "minipro-json-entryPagePath": {
    "prefix": "entryPagePath",
    "body": [
      "\"entryPagePath\": \"pages/index/index\","
    ],
    "url": "https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/app.html#entryPagePath",
    "description": "指定小程序的默认启动路径（首页），常见情景是从微信聊天列表页下拉启动、小程序列表启动等。如果不填，将默认为 pages 列表的第一项。不支持带页面路径参数"
  },
  "minipro-json-pages": {
    "prefix": "pages",
    "body": [
      "\"pages\": [",
      "\t\"pages/index/index\"",
      "],"
    ],
    "url": "https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/app.html#pages",
    "description": "App对应的JSON文件pages配置项"
  },
  "minipro-json-window": {
    "prefix": "window",
    "body": [
      "\"window\": {",
      "\t\"navigationBarBackgroundColor\": \"#000000\",",
      "\t\"navigationBarTextStyle\": \"white\",",
      "\t\"navigationBarTitleText\": \"\",",
      "\t\"navigationStyle\": \"default|custom\",",
      "\t\"backgroundColor\": \"#ffffff\",",
      "\t\"backgroundTextStyle\": \"dark|light\",",
      "\t\"backgroundColorTop\": \"#ffffff\",",
      "\t\"backgroundColorBottom\": \"#ffffff\",",
      "\t\"enablePullDownRefresh\": true,",
      "\t\"onReachBottomDistance\": 50,",
      "\t\"pageOrientation\": \"portrait|auto|landscape\"",
      "},"
    ],
    "url": "https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/app.html#window",
    "description": "App对应的JSON文件window配置项"
  },
  "minipro-json-tabbar": {
    "prefix": "tabbar",
    "body": [
      "\"tabBar\": {",
      "\t\"color\": \"${1}\",",
      "\t\"selectedColor\": \"${2}\",",
      "\t\"backgroundColor\": \"${3}\",",
      "\t\"position\": \"bottom\",",
      "\t\"borderStyle\": \"black\",",
      "\t\"custom\": false,",
      "\t\"list\": [",
      "\t\t{",
      "\t\t\t\"pagePath\": \"${4}\",",
      "\t\t\t\"text\": \"${5}\",",
      "\t\t\t\"iconPath\": \"${6}\",",
      "\t\t\t\"selectedIconPath\": \"${7}\"",
      "\t\t}",
      "\t]",
      "},",
      "${8}"
    ],
    "url": "https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/app.html#tabbar",
    "description": "App对应的JSON文件tabbar配置项"
  },
  "minipro-json-networkTimeout": {
    "prefix": "networkTimeout",
    "body": [
      "\"networkTimeout\": {",
      "\t\"request\": ${1:60000},",
      "\t\"connectSocket\": ${2:60000},",
      "\t\"uploadFile\": ${3:60000},",
      "\t\"downloadFile\": ${4:60000}",
      "},"
    ],
    "url": "https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/app.html#networkTimeout",
    "description": "App对应的JSON文件networktimeout配置项,超时设置"
  },
  "minipro-json-debug": {
    "prefix": "debug",
    "body": [
      "\"debug\": true,"
    ],
    "url": "https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/app.html#debug",
    "description": "App对应的JSON文件debug配置项,是否是调试模式"
  },
  "minipro-json-functionalPages": {
    "prefix": "functionalPages",
    "body": [
      "\"functionalPages\": { \"independent\": false },"
    ],
    "url": "https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/app.html#functionalPages",
    "description": "是否启用插件功能页"
  },
  "minipro-json-subpackages": {
    "prefix": "subpackages",
    "body": [
      "\"subpackages\": [],"
    ],
    "url": "https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/app.html#subpackages",
    "description": "App对应的JSON文件subPackages配置项,分包结构配置"
  },
  "minipro-json-workers": {
    "prefix": "workers",
    "body": [
      "\"workers\": \"\","
    ],
    "url": "https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/app.html#workers",
    "description": "Worker代码放置的目录"
  },
  "minipro-json-requiredBackgroundModes": {
    "prefix": "requiredBackgroundModes",
    "body": [
      "\"requiredBackgroundModes\": [\"audio\"],"
    ],
    "url": "https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/app.html#requiredBackgroundModes",
    "description": "需要在后台使用的能力，如“音乐播放”"
  },
  "minipro-json-plugins": {
    "prefix": "plugins",
    "body": [
      "\"plugins\": {},"
    ],
    "url": "https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/app.html#plugins",
    "description": "使用到的插件"
  },
  "minipro-json-preloadRule": {
    "prefix": "preloadRule",
    "body": [
      "\"preloadRule\": {},"
    ],
    "url": "https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/app.html#preloadRule",
    "description": "分包预下载规则"
  },
  "minipro-json-resizable": {
    "prefix": "resizable",
    "body": [
      "\"resizable\": false,"
    ],
    "url": "https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/app.html#resizable",
    "description": "iPad 小程序是否支持屏幕旋转，默认关闭"
  },
  "minipro-json-usingComponents": {
    "prefix": "usingComponents",
    "body": [
      "\"usingComponents\": {},"
    ],
    "url": "https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/app.html#usingComponents",
    "description": "在此处声明的自定义组件视为全局自定义组件，在小程序内的页面或自定义组件中可以直接使用而无需再声明。"
  },
  "minipro-json-permission": {
    "prefix": "permission",
    "body": [
      "\"permission\": {",
      "\t\"scope.userLocation\": {",
      "\t\t\"desc\": \"你的位置信息将用于小程序位置接口的效果展示\"",
      "\t}",
      "},"
    ],
    "url": "https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/app.html#permission",
    "description": "小程序接口权限相关设置。"
  },
  "minipro-json-sitemapLocation": {
    "prefix": "sitemapLocation",
    "body": [
      "\"sitemapLocation\": \"\","
    ],
    "url": "https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/app.html#sitemapLocation",
    "description": "指明 sitemap.json 的位置"
  },
  "minipro-json-style": {
    "prefix": "style",
    "body": [
      "\"style\": \"\","
    ],
    "url": "https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/app.html#style",
    "description": "指明使用升级后的weui样式"
  },
  "minipro-json-useExtendedLib": {
    "prefix": "useExtendedLib",
    "body": [
      "\"useExtendedLib\": {},"
    ],
    "url": "https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/app.html#useExtendedLib",
    "description": "指明需要引用的扩展库"
  },
  "minipro-json-entranceDeclare": {
    "prefix": "entranceDeclare",
    "body": [
      "\"entranceDeclare\": {},"
    ],
    "url": "https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/app.html#entranceDeclare",
    "description": "微信消息用小程序打开"
  },
  "minipro-json-darkmode": {
    "prefix": "darkmode",
    "body": [
      "\"darkmode\": false,"
    ],
    "url": "https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/app.html#darkmode",
    "description": "小程序支持 DarkMode"
  },
  "minipro-json-themeLocation": {
    "prefix": "themeLocation",
    "body": [
      "\"themeLocation\": \"\","
    ],
    "url": "https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/app.html#themeLocation",
    "description": "指明 theme.json 的位置，darkmode为true必填"
  },
  "minipro-json-lazyCodeLoading": {
    "prefix": "lazyCodeLoading",
    "body": [
      "\"lazyCodeLoading\": \"\","
    ],
    "url": "https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/app.html#lazyCodeLoading",
    "description": "配置自定义组件代码按需注入"
  },
  "minipro-json-singlePage": {
    "prefix": "singlePage",
    "body": [
      "\"singlePage\": {},"
    ],
    "url": "https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/app.html#singlePage",
    "description": "单页模式相关配置"
  },
  "minipro-json-navigationBarBackgroundColor": {
    "prefix": "navigationBarBackgroundColor",
    "body": [
      "\"navigationBarBackgroundColor\": \"#000000\","
    ],
    "url": "https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/page.html",
    "description": "导航栏背景颜色，如 #000000"
  },
  "minipro-json-navigationBarTextStyle": {
    "prefix": "navigationBarTextStyle",
    "body": [
      "\"navigationBarTextStyle\": \"white|black\","
    ],
    "url": "https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/page.html",
    "description": "导航栏标题颜色，仅支持 black / white"
  },
  "minipro-json-navigationBarTitleText": {
    "prefix": "navigationBarTitleText",
    "body": [
      "\"navigationBarTitleText\": \"${1}\","
    ],
    "url": "https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/page.html",
    "description": "导航栏标题文字内容"
  },
  "minipro-json-navigationStyle": {
    "prefix": "navigationStyle",
    "body": [
      "\"navigationStyle\": \"default|custom\","
    ],
    "url": "https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/page.html",
    "description": "导航栏样式，仅支持以下值：default 默认样式custom 自定义导航栏，只保留右上角胶囊按钮。参见注"
  },
  "minipro-json-backgroundColor": {
    "prefix": "backgroundColor",
    "body": [
      "\"backgroundColor\": \"#ffffff\","
    ],
    "url": "https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/page.html",
    "description": "窗口的背景色"
  },
  "minipro-json-backgroundTextStyle": {
    "prefix": "backgroundTextStyle",
    "body": [
      "\"backgroundTextStyle\": \"dark|light\","
    ],
    "url": "https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/page.html",
    "description": "下拉 loading 的样式，仅支持 dark / light"
  },
  "minipro-json-backgroundColorTop": {
    "prefix": "backgroundColorTop",
    "body": [
      "\"backgroundColorTop\": \"#ffffff\","
    ],
    "url": "https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/page.html",
    "description": "顶部窗口的背景色，仅 iOS 支持"
  },
  "minipro-json-backgroundColorBottom": {
    "prefix": "backgroundColorBottom",
    "body": [
      "\"backgroundColorBottom\": \"#ffffff\","
    ],
    "url": "https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/page.html",
    "description": "底部窗口的背景色，仅 iOS 支持"
  },
  "minipro-json-enablePullDownRefresh": {
    "prefix": "enablePullDownRefresh",
    "body": [
      "\"enablePullDownRefresh\": false,"
    ],
    "url": "https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/page.html",
    "description": "是否开启当前页面下拉刷新。"
  },
  "minipro-json-onReachBottomDistance": {
    "prefix": "onReachBottomDistance",
    "body": [
      "\"onReachBottomDistance\": 50,"
    ],
    "url": "https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/page.html",
    "description": "页面上拉触底事件触发时距页面底部距离，单位为px。"
  },
  "minipro-json-pageOrientation": {
    "prefix": "pageOrientation",
    "body": [
      "\"pageOrientation\": \"portrait|auto|landscape\","
    ],
    "url": "https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/page.html",
    "description": "屏幕旋转设置，支持 auto / portrait / landscape"
  },
  "minipro-json-disableScroll": {
    "prefix": "disableScroll",
    "body": [
      "\"disableScroll\": false,"
    ],
    "url": "https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/page.html",
    "description": "设置为 true 则页面整体不能上下滚动。只在页面配置中有效，无法在 app.json 中设置"
  },
  "minipro-json-initialRenderingCache": {
    "prefix": "initialRenderingCache",
    "body": [
      "\"initialRenderingCache\": {},"
    ],
    "url": "https://developers.weixin.qq.com/miniprogram/dev/reference/configuration/page.html",
    "description": "页面初始渲染缓存配置"
  }
}